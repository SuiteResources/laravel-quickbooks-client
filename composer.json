{
    "name": "suiteresources/laravel-quickbooks-client",
    "description": "SuiteResources Laravel Client for QuickBooks.",
    "keywords": [
        "quickbooks",
        "client",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2|^8.0.2",
        "illuminate/database":"^9.0",
        "quickbooks/v3-php-sdk": "^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.4|^9.0",
        "psy/psysh": "^0.10",
        "symfony/thanks": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "SuiteResources\\QuickBooks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SuiteResources\\QuickBooks\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SuiteResources\\QuickBooks\\Providers\\ServiceProvider",
                "SuiteResources\\QuickBooks\\Providers\\ClientServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
